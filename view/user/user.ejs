  <body>
    
    <div class="desktop-5" id="bg_3" >
 
      <img src="s37.jpg" alt="" style="height:100vh; width:100%; position:absolute;">
      <div class="home-parent" id="nav_bar">

        <a href="/home" style="text-decoration: none;"><span class="home" id="home">Home </span></a>
        <a href="/user_events?All=all_events " style="text-decoration: none;"><span class="home" id="home" id="myevents ">My events </span></a>
       <a href="#host_col" style="text-decoration: none;"> <span class="host" id="host" >Host</span></a>
 
    <button class="btn " type="button" id="Profile_btn" onclick="makeAjaxCall3('Profile_btn')" data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight" aria-controls="offcanvasRight">
     
      <div class="wrapper">
        <img
          class="ic2"
          loading="eager"
          id="profile_img"
          alt=""
          src="profile.png"
        
        /> 
  </div>
</button>

    <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasRight" aria-labelledby="offcanvasRightLabel" style="padding:0;">
      <img src="s43.jpg" alt="" style="height:100vh; width:100%; position: absolute; z-index: 0;">
      <div class="offcanvas-header">
        <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close" style="margin-right: 0.5rem; background-color:white; position:absolute; top:1.5rem; right:1rem"></button>
      </div>
      <div id="profile_img2">
        <div class="wrapper2">
          <img class="icon2" loading="eager" id="profile_img" alt="" src="profile.png" />
          <h5 class="full_name" style="color:white"><%= user_name %></h5>
      </div>
      </div>
      <div class="offcanvas-body" style=" margin: 0;">
        <ul class="menu" style="color:white">
        
          <!-- <li class="menue_1"><a href="#">Tickets</a></li>
          <li class="menue_1"><a href="#">Settings</a></li>
          <li class="menue_1"><a href="#">Logout</a></li> -->
      </ul>
      </div>
      <div class="d-grid gap-2 logout" style="position:relative; bottom: 20%;">
       <a href="/home"> <button class="btn btn-success" type="button" style=" height:3rem; width:150%; position:relative; top:3rem" onclick="makeAjax('logout')">Log out</button></a>
       
      </div> 
    </div>

        
      </div>
      <div class="frame-parent" id="main_content" style="left:0 ;top:28%">
        <div class="hi-suprith-wrapper">
          <h1 class="hi-suprith" id="main_content_title">Hi <%=user_name.substring(0,1).toUpperCase()+ user_name.substring(1,user_name.length).toLowerCase().trim()+"..."%></h1>
        </div>
        <div class="welcome-to-event-horizon-onlin-wrapper">
          <b class="welcome-to-event-container" id="content_body">
            <span class="welcome-to-event-container1">
              <p class="welcome-to-event">
                Welcome to Event Horizon Online, where every click opens the
                door to a world of seamless event experiences. Embrace the
                extraordinary, as we navigate the realms of virtual celebration
                together."
              </p>
              <p class="blank-line">&nbsp;</p>
            </span>
          </b>
        </div>
      </div>
      <div class="instance-parent" id="Brand_logo">
        <div class="vector-parent" id="logo_title">
          <img class="instance-child" alt="" src="b_name_e.png" />

          <img class="instance-item" alt="" src="b_name_h.png" />
        </div>
        <label class="event-horizon-wrapper">
          <div class="event-horizon">Event Horizon</div>
        </label>
